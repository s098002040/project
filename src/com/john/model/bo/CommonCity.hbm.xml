<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.john.model.bo.CommonCity" table="CommonCity">
    <id name="id" type="java.lang.Integer" column="id">
      <generator class="native"/>
    </id>

    <property name="cityCode" type="java.lang.String" column="cityCode" not-null="true" length="10" />
    <property name="cityName" type="java.lang.String" column="cityName" not-null="true" length="10" />
</class>
</hibernate-mapping>
